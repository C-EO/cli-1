-- Generated by Supabase CLI

<% props.columns.forEach((column) => { %>
/*   
 * <%= column.name %>  
*/
alter table "<%= props.table.schema %>"."<%= props.table.name %>" add column 
"<%= column.name %>"                
    <%= column.data_type %> -- type
    default <%= column.default_value %> -- default
;
comment on column "<%= props.table.schema %>"."<%= props.table.name %>"."<%= column.name %>" is 
"<%= column.comment %>";
<% }) %>


